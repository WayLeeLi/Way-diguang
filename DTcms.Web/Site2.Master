        $(document).ready(function () {
            $("#DivPublish").hide();
            $(".sousuo_22").each(function () {
                var chushi = $(this).val();
                $(this).focus(function () {
                    if ($(this).value == "") {
                        $(this).val(chushi);
                    } else {
                        $(this).val("");
                    }
                })
                $(this).blur(function () {
                    if ($(this).val() == "") {
                        $(this).val(chushi);
                    }
                })
            });
            $("#BuyPublish").mouseover(function () {
                $("#DivPublish").show();
            });
            //$("#BuyPublish").mouseout(function () {
            // $("#DivPublish").hide();
            //  });
        })
        function showPublish() {
            $("#DivPublish").show();
        }
    </script>
